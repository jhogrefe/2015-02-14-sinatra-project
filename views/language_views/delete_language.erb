<h1><div class="transbox">Delete Language</div></h1>

<p>
Select which language you would like to delete.
</p>

  <form action="/deleted_language" method="post">
    <select name="lang_id">
      <% Lang.all.each do |l| %>
        <option value="<%= l.id %>"><%= l.id%> - <%= l.lang %></option>
      <% end %>
    </select>
    <br><br>
    <input type="submit" value="Submit" class="btn">
  </form>
